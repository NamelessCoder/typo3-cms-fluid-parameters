{
    "name": "namelesscoder/typo3-cms-fluid-parameters",
    "description": "Proof of concept for parameterised Fluid templates",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "NamelessCoder\\FluidParameters\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NamelessCoder\\FluidParameters\\Tests": "Tests/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "typo3/cms-core": "^8.4.1|^9|dev-master"
    },
    "replace": {
        "fluid_parameters": "self.version"
    }
}
